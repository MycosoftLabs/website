# =============================================================================
# Mycosoft Website Environment Variables
# =============================================================================
# Copy this file to .env.local and fill in the values
# NEVER commit .env.local to version control
# =============================================================================

# =============================================================================
# SUPABASE (Required)
# =============================================================================
NEXT_PUBLIC_SUPABASE_URL=https://your-project.supabase.co
NEXT_PUBLIC_SUPABASE_ANON_KEY=your-anon-key
SUPABASE_SERVICE_ROLE_KEY=your-service-role-key

# =============================================================================
# MINDEX API (Required for species database)
# =============================================================================
# Local: use localhost. For dev-using-VMs (no local DB): use MINDEX VM (see block below)
MINDEX_API_BASE_URL=http://localhost:8000
MINDEX_API_URL=http://localhost:8000
MINDEX_API_KEY=local-dev-key
# MINDEX_DATABASE_URL=postgresql://user:pass@localhost:5432/mindex  # Optional direct DB access

# =============================================================================
# MYCORRHIZAE PROTOCOL (Required for MINDEX streaming/publish)
# =============================================================================
# For dev/sandbox: get keys from internal keys API (see docs/DEV_SANDBOX_KEYS_FLOW_FEB06_2026.md)
#   curl -s http://localhost:3010/api/internal/keys/dev (with X-Internal-Keys-Secret if set)
MYCORRHIZAE_API_URL=http://localhost:8002
# Sandbox/VM: MYCORRHIZAE_API_URL=http://192.168.0.187:8002
MYCORRHIZAE_PUBLISH_KEY=
MYCORRHIZAE_ADMIN_KEY=

# =============================================================================
# GOOGLE MAPS (Required for maps)
# =============================================================================
NEXT_PUBLIC_GOOGLE_MAPS_API_KEY=your-google-maps-api-key
# Ensure these APIs are enabled in Google Cloud Console:
# - Maps JavaScript API
# - Geocoding API
# - Places API

# =============================================================================
# GOOGLE OAUTH (Required for Google Sign-In)
# =============================================================================
GOOGLE_CLIENT_ID=your-client-id.apps.googleusercontent.com
GOOGLE_CLIENT_SECRET=your-client-secret

# =============================================================================
# GITHUB OAUTH (Required for GitHub Sign-In)
# =============================================================================
GITHUB_CLIENT_ID=your-github-client-id
GITHUB_CLIENT_SECRET=your-github-client-secret

# =============================================================================
# NEXTAUTH (Required for authentication)
# =============================================================================
NEXTAUTH_URL=http://localhost:3000
NEXTAUTH_SECRET=your-super-secret-key-at-least-32-characters

# =============================================================================
# STRIPE (Required for payments)
# =============================================================================
STRIPE_SECRET_KEY=sk_test_your-stripe-secret-key
NEXT_PUBLIC_STRIPE_PUBLISHABLE_KEY=pk_test_your-stripe-publishable-key
STRIPE_WEBHOOK_SECRET=whsec_your-webhook-secret

# =============================================================================
# AI PROVIDERS
# =============================================================================
# OpenAI
OPENAI_API_KEY=sk-your-openai-key
OPENAI_PERSONAL_API_KEY=sk-your-personal-openai-key

# Anthropic Claude
ANTHROPIC_API_KEY=sk-ant-api03-your-anthropic-key

# Groq
GROQ_API_KEY=gsk_your-groq-key

# xAI Grok
XAI_API_KEY=your-xai-key

# Google AI (Gemini)
GOOGLE_AI_API_KEY=your-google-ai-key

# =============================================================================
# RESEARCH APIS
# =============================================================================
# iNaturalist
INATURALIST_JWT=your-inaturalist-jwt-token

# NIH/PubMed
NIH_API_KEY=your-nih-api-key

# Elsevier
ELSEVIER_API_KEY=your-elsevier-api-key

# =============================================================================
# BLOCKCHAIN
# =============================================================================
INFURA_API_KEY=your-infura-api-key
QUICKNODE_SOLANA_ENDPOINT=https://your-quicknode-endpoint

# =============================================================================
# INFRASTRUCTURE
# =============================================================================
# UniFi Network
UNIFI_API_KEY=your-unifi-api-key
UNIFI_HOST=192.168.0.1

# Proxmox
PROXMOX_API_TOKEN=your-proxmox-token
PROXMOX_HOST=your-proxmox-host

# =============================================================================
# TRACKING APIs
# =============================================================================
FLIGHTRADAR24_API_KEY=your-flightradar24-key

# =============================================================================
# COMMUNICATION
# =============================================================================
DISCORD_BOT_TOKEN=your-discord-bot-token

# =============================================================================
# AUTOMATION
# =============================================================================
N8N_API_KEY=your-n8n-api-key
N8N_WEBHOOK_URL=http://localhost:5678

# =============================================================================
# LOCAL DEV USING REMOTE VMs (no local database or agents)
# =============================================================================
# Uncomment and use when you want the dev server (localhost:3010) to call MAS and
# MINDEX on the VMs. See docs/VM_LAYOUT_AND_DEV_REMOTE_SERVICES_FEB06_2026.md
# MAS_API_URL=http://192.168.0.188:8001
# MINDEX_API_URL=http://192.168.0.189:8000
# MINDEX_API_BASE_URL=http://192.168.0.189:8000
# NEXT_PUBLIC_MINDEX_URL=http://192.168.0.189:8000
# MYCORRHIZAE_API_URL=http://192.168.0.187:8002
# REDIS_URL=redis://192.168.0.187:6379
# N8N_URL=http://192.168.0.188:5678
# NEXT_PUBLIC_BASE_URL=http://localhost:3010
# Optional: protect internal keys API (dev/test/sandbox key generation)
# INTERNAL_KEYS_ADMIN_SECRET=your-secret

# =============================================================================
# MAS (Multi-Agent System) Configuration
# =============================================================================
# MAS VM: 192.168.0.188. Use these so dev site uses remote orchestrator (no local agents).
MAS_API_URL=http://192.168.0.188:8001

# Public MAS API URL (for client-side API calls)
NEXT_PUBLIC_MAS_API_URL=http://192.168.0.188:8001

# WebSocket URL for real-time topology updates
NEXT_PUBLIC_MAS_WS_URL=ws://192.168.0.188:8001/api/dashboard/ws
# Set to "false" to disable WebSocket and use polling instead
NEXT_PUBLIC_MAS_WS_ENABLED=true

# Server-Sent Events URL for log streaming
NEXT_PUBLIC_MAS_SSE_URL=http://192.168.0.188:8001/api/dashboard/stream
# Set to "false" to disable SSE log streaming
NEXT_PUBLIC_MAS_SSE_ENABLED=true

# =============================================================================
# OPTIONAL: CLOUD PROVIDERS (for production)
# =============================================================================
# AWS Bedrock
# AWS_BEDROCK_ACCESS_KEY=your-aws-access-key
# AWS_BEDROCK_SECRET_KEY=your-aws-secret-key
# AWS_REGION=us-east-1

# Azure OpenAI
# AZURE_OPENAI_API_KEY=your-azure-openai-key
# AZURE_OPENAI_ENDPOINT=https://your-endpoint.openai.azure.com

# Google Vertex AI
# GOOGLE_VERTEX_API_KEY=your-vertex-key
# GOOGLE_CLOUD_PROJECT=your-project-id
